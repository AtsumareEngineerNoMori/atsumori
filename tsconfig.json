{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "strict": true,
    "jsx": "preserve",
    "moduleResolution": "node",

    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },

  },
  "include": [
    "src/**/*.ts",
    "src/**/*.vue",
    "src/components/**/*.vue",
    "src/components/date/*.vue",

  ],
  "exclude": ["node_modules"]
}
